#include <stdio.h>

int main() {
    int n=6 ; // Example value for n

    for (int i = 1; i <= n; i++) {       // Outer loop runs n times
        for (int j = 1; j <= i; j++) {   // Inner loop runs n times
            printf("*"); // Print statement, n * n times in total.......0(n^2)
        }
        printf("\n");
    }

    return 0;
}

